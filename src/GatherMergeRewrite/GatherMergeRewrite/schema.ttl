
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix nuget: <http://nuget.org/schema#> .

nuget:PackageRegistrationResource rdfs:subClassOf nuget:Resource .

nuget:PackageResource rdfs:subClassOf nuget:Resource .

nuget:OwnerResource rdfs:subClassOf nuget:Resource .

nuget:registration rdfs:domain nuget:PackageResource ;
                   rdfs:range nuget:PackageRegistrationResource .

nuget:registration owl:inverseOf nuget:package .

nuget:owner rdfs:domain nuget:PackageRegistrationResource ;
            rdfs:range nuget:OwnerResource .

nuget:owner owl:inverseOf nuget:owns .

